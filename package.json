{
  "name": "grupytinder",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "test": "jest --coverage -u",
    "start": "expo start",
    "android": "expo run:android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@firebase/auth": "^1.7.9",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^21.2.0",
    "@react-native-firebase/auth": "^21.2.0",
    "@react-native-firebase/crashlytics": "^21.2.0",
    "@react-native-firebase/firestore": "^21.2.0",
    "@react-native/gradle-plugin": "^0.76.5",
    "@react-navigation/core": "^7.3.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "^52.0.27",
    "expo-auth-session": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-dev-client": "~5.0.10",
    "expo-location": "~18.0.10",
    "expo-modules-core": "~2.1.2",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "firebase": "^11.0.1",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.3",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.77.0",
    "react-native-css-interop": "^0.1.22",
    "react-native-deck-swiper": "^2.0.17",
    "react-native-gesture-handler": "^2.23.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.6.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-runtime": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/runtime": "^7.26.7",
    "@eslint/js": "^9.21.0",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.9.0",
    "@types/react": "18.3.1",
    "@types/react-test-renderer": "18.3.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0",
    "postcss": "^8.5.1",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.7.2"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ],
    "moduleNameMapper": {
      "^screens/(.*)$": "<rootDir>/screens/$1"
    },
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js",
      "<rootDir>/__tests__/**/*.test.jsx",
      "<rootDir>/__tests__/**/*.test.tsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|expo-asset|expo-constants|@react-native|expo-font|expo|expo-location|@expo|@unimodules|@react-navigation|react-native-screens|react-native-safe-area-context|nativewind|@react-native-async-storage/async-storage|react-native-css-interop|@testing-library/react-native|expo-modules-core)/)"
    ]
  },
  "private": true
}
