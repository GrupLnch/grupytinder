{
  "name": "grupytinder",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@firebase/auth": "^1.8.1",
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-native-firebase/app": "^21.5.0",
    "@react-native-firebase/auth": "^21.5.0",
    "@react-native-firebase/crashlytics": "^21.5.0",
    "@react-native-firebase/firestore": "^21.5.0",
    "@react-native/gradle-plugin": "^0.76.2",
    "@react-navigation/core": "^7.0.3",
    "@react-navigation/native": "^7.0.3",
    "@react-navigation/native-stack": "^7.0.4",
    "@react-navigation/routers": "^7.1.1",
    "expo": "^52.0.7",
    "expo-auth-session": "^6.0.0",
    "expo-build-properties": "~0.13.1",
    "expo-dev-client": "~5.0.2",
    "expo-modules-core": "~2.0.3",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "firebase": "^11.0.2",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.1.0",
    "styled-components": "^6.1.13",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17"
  },
  "overrides": {
    "@firebase/auth": {
      "@react-native-async-storage/async-storage": "2.1.0"
    }
  },
  "resolutions": {
    "@react-native/gradle-plugin": "^0.76.2"
  },
  "private": true
}
